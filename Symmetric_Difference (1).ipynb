      "source": [
        "# Find the Symmetric Difference\n",
        "def sym(args):\n",
        "\n",
        "  pasado = []\n",
        "  salida = []\n",
        "\n",
        "  for list in args:\n",
        "     for x in list:\n",
        "        if x in salida:\n",
        "            salida.remove(x)\n",
        "            pasado.append(x)\n",
        "        elif ~ (x in pasado):\n",
        "            salida.append(x)\n",
        "\n",
        "  return salida\n",
        "\n"
      ]
    }
  ]
}